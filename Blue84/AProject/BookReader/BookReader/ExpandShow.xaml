				<RotateTransform x:Name="RTF_Buffering" Angle="-180"/>
			</Ellipse.RenderTransform>
			<Ellipse.Fill>
				<ImageBrush Stretch="Fill" ImageSource="Pictures/ExpanderShow/Preloader128x128.png"/>
			</Ellipse.Fill>
		</Ellipse>
