<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:WpfCalendar.View"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:UpBan="clr-namespace:UpBan;assembly=UpBan" x:Class="WpfCalendar.View.MainViewer"
             x:Name="UserControl"
             Height="480"
             Width="800"
             mc:Ignorable="d"
             d:DesignWidth="640"
             d:DesignHeight="480">
	<Grid x:Name="LayoutRoot">
		<Grid.Background>
			<ImageBrush ImageSource="/WpfCalendar;component/Pictures/MainView/mainBack.jpg" />
		</Grid.Background>
		<Grid Margin="17,103,17,8">
			<local:MyCalendar />
		</Grid>
      
		<UpBan:UpBanControl VerticalAlignment="Top" d:LayoutOverrides="Height"/>
      
	</Grid>
</UserControl>